<?xml version="1.0" encoding="UTF-8"?><!--jaemzware december 28th, 2016--><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform"><xsl:template match="/"><html><body><!--PAGE HEADER--><h1>Posts</h1><table border="1"><tr bgcolor="#9acd32"><!--TABLE COLUMN HEADERS--><th style="text-align:left; vertical-align: text-top;">Title</th><th style="text-align:left; vertical-align: text-top;">Post</th></tr><!--TOP LEVEL TABLE DATA XML ITEM--><xsl:for-each select="//item">    <xsl:sort select="link"/><tr>    <!--TABLE DATA XML CHILD ITEMS-->    <td style="text-align:left;"><xsl:value-of select="title"/></td>    <td style="text-align:left;"><xsl:value-of select="link"/></td></tr><tr>    <td colspan="2">        CONTENT ENCODED:    </td></tr><tr>    <!--TABLE DATA XML CHILD ITEMS-->    <td colspan="2">        <xsl:value-of select="*[name()='content:encoded']/text()"/>    </td></tr></xsl:for-each></table></body></html></xsl:template></xsl:stylesheet>