<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform"><xsl:template match="/"><html><body><!--TOP LEVEL DATA XML ITEM item FROM/TO XML--><xsl:for-each select="//item">    <xsl:sort select="title"/>    <!-- <h1> TO XML-->    <xsl:text>&lt;h1&gt;</xsl:text>    <!-- DATA XML CHILD ITEM title FROM/TO XML-->    <xsl:value-of select="title"/>    <!-- </h1> TO XML-->    <xsl:text>&lt;/h1&gt;</xsl:text>    <!-- <a href=" TO XML-->    <xsl:text>&lt;a href="</xsl:text>    <!-- DATA XML CHILD ITEM link FROM/TO XML-->    <xsl:value-of select="link"/>    <!-- " target="_blank"> TO XML-->    <xsl:text>" target="_blank"&gt;</xsl:text>    <!-- DATA XML CHILD ITEM link FROM/TO XML-->    <xsl:value-of select="link"/>    <!-- "></a> TO XML-->    <xsl:text>&lt;/a&gt;</xsl:text>    <!-- <br /> TO XML-->    <xsl:text>&lt;br /&gt;</xsl:text>    <!-- DATA XML CHILD ITEM content:encoded FROM/TO XML-->    <xsl:value-of select="*[name()='content:encoded']/text()"/></xsl:for-each></body></html></xsl:template></xsl:stylesheet>